<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.control.TextField?>

<AnchorPane xmlns="http://javafx.com/javafx/17" prefHeight="580.0" prefWidth="560.0" xmlns:fx="http://javafx.com/fxml/1" fx:controller="jrl.qam2final.Controller.CustomerCreateController">
    <children>
        <TextField fx:id="newCustomerAddressTextField" layoutX="220.0" layoutY="190.0" prefHeight="26.0" prefWidth="210.0" promptText="Address" text="Address" />
        <Label fx:id="newCustomerAddressLabel" layoutX="70.0" layoutY="190.0" text="Address">
        </Label>
        <TextField fx:id="newCustomerPostalCodeTextField" layoutX="220.0" layoutY="250.0" prefHeight="26.0" prefWidth="210.0" promptText="Postal Code" text="Postal Code" />
        <Label fx:id="newCustomerPostalCodeLabel" layoutX="70.0" layoutY="250.0" text="Postal Code">
        </Label>
        <TextField fx:id="newCustomerPhoneNumberTextField" layoutX="220.0" layoutY="310.0" prefHeight="26.0" prefWidth="210.0" promptText="Phone Number" text="Phone Number" />
        <Label fx:id="newCustomerPhoneNumberLabel" layoutX="70.0" layoutY="310.0" text="Phone Number">
        </Label>
        <Label fx:id="newCustomerCountryLabel" layoutX="70.0" layoutY="370.0" text="Country">
        </Label>
        <TextField fx:id="newCustomerIdTextField" disable="true" layoutX="220.0" layoutY="70.0" prefHeight="26.0" prefWidth="210.0" promptText="Disabled" />
        <Label fx:id="newCustomerIdLabel" layoutX="70.0" layoutY="70.0" text="Customer ID">
        </Label>
        <TextField fx:id="newCustomerNameTextField" layoutX="220.0" layoutY="130.0" prefHeight="26.0" prefWidth="210.0" promptText="Customer Name" text="Name" />
        <Label fx:id="newCustomerNameLabel" layoutX="70.0" layoutY="130.0" text="Name">
        </Label>
        <ComboBox fx:id="newCustomerCountryComboBox" onAction="#onActionDivisionFromCountryCCC" layoutX="220.0" layoutY="370.0" prefWidth="210.0" promptText="Country" visibleRowCount="5" />
        <Label fx:id="newCustomerDivisionLabel" layoutX="70.0" layoutY="430.0" text="Division">
        </Label>
        <ComboBox fx:id="newCustomerDivisionComboBox" layoutX="220.0" layoutY="430.0" prefWidth="210.0" promptText="Division" visibleRowCount="5" />
        <Button fx:id="newCustomerSaveButton" layoutX="70.0" layoutY="480.0" mnemonicParsing="false" onAction="#onActionSaveCreatedCustomerCCC" prefHeight="26.0" prefWidth="80.0" text="Save" />
        <Button fx:id="newCustomerCancelButton" layoutX="170.0" layoutY="480.0" mnemonicParsing="false" onAction="#onActionCancelCreatedCustomerCCC" prefHeight="26.0" prefWidth="80.0" text="Cancel" />
        <Label fx:id="newCustomerHeaderLabel" prefWidth="260.0" layoutX="70.0" layoutY="20.0" text="New Customer">
            <font>
                <Font size="20.0" />
            </font>
        </Label>
    </children>
</AnchorPane>