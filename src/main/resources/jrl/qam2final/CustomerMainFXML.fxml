<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.control.TableColumn?>

<AnchorPane xmlns="http://javafx.com/javafx/17" prefHeight="540.0" prefWidth="1180.0" xmlns:fx="http://javafx.com/fxml/1" fx:controller="jrl.qam2final.Controller.CustomerMainController">
    <children>
        <Button fx:id="onActionCMCToCCCButton" maxHeight="-Infinity" maxWidth="-Infinity" mnemonicParsing="false" onAction="#onActionNavigateCustomerCreateCMC" layoutX="50.0" layoutY="72.0" prefHeight="26.0" prefWidth="160.0" text="New Customer" />
        <Button fx:id="onActionCMCToCUCButton" maxHeight="-Infinity" maxWidth="-Infinity" mnemonicParsing="false" onAction="#onActionNavigateCustomerUpdateCMC" layoutX="220.0" layoutY="72.0" prefHeight="26.0" prefWidth="160.0" text="Update Customer" />
        <Button fx:id="onActionCustomerDeleteButton" maxHeight="-Infinity" maxWidth="-Infinity" mnemonicParsing="false" onAction="#onActionDeleteCustomerCMC" layoutX="390.0" layoutY="72.0" prefHeight="26.0" prefWidth="160.0" text="Delete Customer" />
        <Button fx:id="onActionCMCToAMCButton" layoutX="630.0" layoutY="72.0" mnemonicParsing="false" onAction="#onActionNavigateAppointmentMainCMC" prefHeight="26.0" prefWidth="160.0" text="Appointment Main" />
        <Button fx:id="onActionCMCToRMCButton" layoutX="800.0" layoutY="72.0" mnemonicParsing="false" onAction="#onActionNavigateReportMainCMC" prefHeight="26.0" prefWidth="160.0" text="Reports Main" />
        <Button fx:id="onActionExitButton" layoutX="970.0" layoutY="72.0" contentDisplay="RIGHT" mnemonicParsing="false" onAction="#onActionExitCMC" prefHeight="26.0" prefWidth="160.0" text="Exit" />
        <TableView fx:id="customerMainTableView" layoutX="50.0" layoutY="128.0" prefHeight="400.0" prefWidth="1080.0">
            <columns>
                <TableColumn fx:id="customerMainIdColumn" prefWidth="180.0" text="Customer ID" />
                <TableColumn fx:id="customerMainNameColumn" prefWidth="180.0" text="Customer Name" />
                <TableColumn fx:id="customerMainAddressColumn" prefWidth="180.0" text="Address" />
                <TableColumn fx:id="customerMainPostalCodeColumn" prefWidth="180.0" text="Postal Code" />
                <TableColumn fx:id="customerMainPhoneNumberColumn" prefWidth="180.0" text="Phone Number" />
                <TableColumn fx:id="customerMainDivisionColumn" prefWidth="180.0" text="State/Province" />
            </columns>
        </TableView>
        <Label fx:id="customerMainTitleLabel" layoutX="530.0" layoutY="30.0" prefHeight="27.0" prefWidth="260.0" text="Customer Main">
            <font>
                <Font size="20.0" />
            </font>
        </Label>
    </children>
</AnchorPane>